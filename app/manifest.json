{
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "description": "__MSG_appDescription__",
  "version": "1.29.10.56",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "128": "images/icon-in-128.png"
  },
  "background": {
    "scripts": [
      "scripts/eventPage.js"
    ],
    "persistent": true
  },
  "content_scripts": [{
    "matches": [
      "https://*.linkedin.com/in/*",
      "https://*.linkedin.com/profile/*"
    ],
    "css": [
      "styles/contentscript.css",
      "bower_components/sweetalert2/dist/sweetalert2.min.css"
    ],
    "js": [
      "scripts/contentscript.js"
    ],
    "run_at": "document_end",
    "all_frames": true
  }],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; connect-src 'self';",
  "homepage_url": "http://www.recmanplugins.no",
  "minimum_chrome_version": "50.0",
  "offline_enabled": false,
  "optional_permissions": [
    "contextMenus"
  ],
  "current_locale": "en",
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "*://*.recmanplugins.no/*",
      "*://*.recman.no/*",
      "*://*.linkedin.com/*",
      "*://*.licdn.com/*",
      "*://*.intercomcdn.com/*",
      "*://*.helpcrunch.com/*",
      "*://*.googletagmanager.com/*",
      "http://localhost:9007/*"
    ]
  },
  "page_action": {
    "default_title": "__MSG_browserActionTitle__"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "background",
    "bookmarks",
    "browsingData",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "debugger",
    "declarativeContent",
    "desktopCapture",
    "downloads",
    "fontSettings",
    "gcm",
    "geolocation",
    "history",
    "identity",
    "idle",
    "management",
    "nativeMessaging",
    "notifications",
    "pageCapture",
    "power",
    "printerProvider",
    "privacy",
    "proxy",
    "sessions",
    "storage",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.storage",
    "tabCapture",
    "tabs",
    "topSites",
    "tts",
    "ttsEngine",
    "unlimitedStorage",
    "webNavigation",
    "webRequestBlocking",
    "http://*/",
    "https://*/",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "_locales/*",
    "bower_components/*",
    "images/*",
    "scripts/*",
    "styles/*"
  ]
}
